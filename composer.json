{
    "name": "snipershady/ratelimiter",
    "description": "A free and easy-to-use rate limiter",
    "keywords": ["ratelimiter", "ratelimit", "ban"],
    "homepage": "https://www.spinfo.it",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Stefano Perrini",
            "email": "perrini.stefano@gmail.com",
            "homepage": "https://www.spinfo.it"
        }
    ],
    "autoload": {
        "psr-4": {
            "RateLimiter\\": "src/"
        }       
    },
    "autoload-dev": {
        "psr-4": {
            "RateLimiter\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1",
        "ext-apcu": "*",
        "ext-redis": "*",
        "ext-curl": "*",
        "predis/predis": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.8"
    }
}
